<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bgcolor"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_ir_intent_explicito_parametros"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="192dp"
        android:text="Param Intent"
        app:layout_constraintEnd_toEndOf="@+id/btn_ir_ciclo_vida"
        app:layout_constraintHorizontal_bias="0.222"
        app:layout_constraintStart_toStartOf="@+id/btn_ir_ciclo_vida"
        app:layout_constraintTop_toBottomOf="@+id/btn_ir_ciclo_vida" />

    <Button
        android:id="@+id/btn_ir_intent_con_respuesta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="268dp"
        android:text="1. Respuesta"
        app:layout_constraintEnd_toEndOf="@+id/btn_ir_ciclo_vida"
        app:layout_constraintHorizontal_bias="0.3"
        app:layout_constraintStart_toStartOf="@+id/btn_ir_ciclo_vida"
        app:layout_constraintTop_toBottomOf="@+id/btn_ir_ciclo_vida" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginTop="237dp"
        android:text="Que tal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_ir_ciclo_vida"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:text="Ciclo Vida"
        app:layout_constraintBottom_toBottomOf="@+id/textView29"
        app:layout_constraintStart_toEndOf="@+id/textView29"
        app:layout_constraintTop_toTopOf="@+id/textView29"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/btn_ir_list_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="88dp"
        android:text="List View"
        app:layout_constraintEnd_toEndOf="@+id/btn_ir_ciclo_vida"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btn_ir_ciclo_vida"
        app:layout_constraintTop_toBottomOf="@+id/btn_ir_ciclo_vida" />

    <Button
        android:id="@+id/btn_ir_recycler_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="360dp"
        android:text="R. View"
        app:layout_constraintEnd_toEndOf="@+id/btn_ir_ciclo_vida"
        app:layout_constraintHorizontal_bias="0.357"
        app:layout_constraintStart_toStartOf="@+id/btn_ir_ciclo_vida"
        app:layout_constraintTop_toBottomOf="@+id/btn_ir_ciclo_vida" />
</androidx.constraintlayout.widget.ConstraintLayout>